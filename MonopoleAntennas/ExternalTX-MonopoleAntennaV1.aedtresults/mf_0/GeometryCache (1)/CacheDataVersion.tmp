$begin 'CacheDataVersion'
	DataVersion=12
$end 'CacheDataVersion'
$begin 'VariableValues'
	OuterCoaxDiameter='4.12mm'
	TubingThickness='2mm'
	TubingRadius='3in'
	TubingHeight='200mm'
	AntennaTubingSpace='2mm'
	InnerCoaxDiameter='1.12mm'
	AntennaCoverOffset='AntennaRadius*cos(MonopoleLength/AntennaRadius) meter'
	DielectricCoaxDiameter='3mm'
	AntennaWireRadius='0.6mm'
	Period='1.1e-09s'
	AntennaCoverAngle='180 - (DipoleLength/(2*pi*(TubingRadius+TubingThickness + AntennaTubingSpace)))*360'
	SpeedOfLight='300000000m_per_sec'
	DipoleLength='((SpeedOfLight*Period)/2) meter'
	RadiatingSurface_X='300mm'
	RadiatingSurface_Z='200mm'
	RadiatingSurface_Y='200mm'
	AntennaRadius='TubingRadius + TubingThickness + AntennaTubingSpace'
	MonopoleLength='SpeedOfLight*Period/2'
$end 'VariableValues'
